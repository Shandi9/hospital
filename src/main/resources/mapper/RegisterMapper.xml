<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.hospital.mapper.RegisterMapper">

    <resultMap id="BaseResultMap" type="com.example.hospital.domain.Register">
            <id property="registerId" column="register_id" jdbcType="INTEGER"/>
            <result property="registerPid" column="register_pid" jdbcType="INTEGER"/>
            <result property="registerType" column="register_type" jdbcType="VARCHAR"/>
